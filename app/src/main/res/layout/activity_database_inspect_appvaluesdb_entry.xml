<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="mjt.shopper.MainActivity">
    <LinearLayout
        android:id="@+id/adiavll"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="@dimen/standard_heading_height">
        <TextView
            android:id="@+id/adi_appvaluesdb_valueid"
            android:layout_width="@dimen/standard_dummy_size"
            android:layout_height="@dimen/standard_subsubsubheading_height"
            android:layout_weight="0.1"
            android:gravity="center_vertical"
            android:textSize="@dimen/standard_subsubsubheading_text_size"
            android:textColor="@color/colorRequiredLabel"/>
        <TextView
            android:layout_width="@dimen/standard_dummy_size"
            android:layout_height="match_parent"
            android:layout_weight="0.02"/>
        <TextView
            android:id="@+id/adi_appvaluesdb_valuename"
            android:layout_width="@dimen/standard_dummy_size"
            android:layout_height="@dimen/standard_subsubsubheading_height"
            android:layout_weight="0.25"
            android:gravity="center_vertical"
            android:singleLine="true"
            android:textStyle="bold"/>
        <TextView
            android:layout_width="@dimen/standard_dummy_size"
            android:layout_height="match_parent"
            android:layout_weight="0.02"/>
        <TextView
            android:id="@+id/adi_appvaluesdb_valuetype"
            android:layout_width="@dimen/standard_dummy_size"
            android:layout_height="@dimen/standard_subsubsubheading_height"
            android:layout_weight="0.25"
            android:gravity="center_vertical"
            android:singleLine="true" />
        <TextView
            android:layout_width="@dimen/standard_dummy_size"
            android:layout_height="match_parent"
            android:layout_weight="0.02"/>
        <TextView
            android:id="@+id/adi_appvaluesdb_valueint"
            android:layout_width="@dimen/standard_dummy_size"
            android:layout_height="@dimen/standard_subsubsubheading_height"
            android:layout_weight="0.15"
            android:gravity="center_vertical"
            android:singleLine="true" />
        <TextView
            android:layout_width="@dimen/standard_dummy_size"
            android:layout_height="match_parent"
            android:layout_weight="0.02"/>
        <TextView
            android:id="@+id/adi_appvaluesdb_valuereal"
            android:layout_width="@dimen/standard_dummy_size"
            android:layout_height="@dimen/standard_subsubsubheading_height"
            android:layout_weight="0.25"
            android:gravity="center_vertical"
            android:singleLine="true" />
        <TextView
            android:layout_width="@dimen/standard_dummy_size"
            android:layout_height="match_parent"
            android:layout_weight="0.02"/>
        <TextView
            android:id="@+id/adi_appvaluesdb_valuestr"
            android:layout_width="@dimen/standard_dummy_size"
            android:layout_height="@dimen/standard_subsubsubheading_height"
            android:layout_weight="0.25"
            android:gravity="center_vertical"
            android:singleLine="true" />
        <TextView
            android:layout_width="@dimen/standard_dummy_size"
            android:layout_height="match_parent"
            android:layout_weight="0.02"/>
        <TextView
            android:id="@+id/adi_appvaluesdb_valueincludeinsettings"
            android:layout_width="@dimen/standard_dummy_size"
            android:layout_height="@dimen/standard_subsubsubheading_height"
            android:layout_weight="0.05"
            android:gravity="center_vertical"
            android:singleLine="true" />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/adiavll2"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="@dimen/standard_heading_height">
        <TextView
            android:id="@+id/adi_appvaluesdb_settingsinfo"
            android:layout_width="0dp"
            android:layout_height="@dimen/standard_subsubsubheading_height"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:singleLine="true" />
    </LinearLayout>
</LinearLayout>